{
    "extension": [
        ".ts"
    ],
    "require": [
        "ts-node/register/transpile-only"
    ],
    "reporter": [
        "lcov",
        "text",
        "text-summary",
        "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "exclude": [
        "src/utils/test",
        "src/**/*.test.ts"
    ]
}
