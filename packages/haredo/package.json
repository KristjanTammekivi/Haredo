{
    "name": "haredo",
    "version": "3.3.2",
    "description": "A type-safe client library for rabbitmq/amqp",
    "homepage": "https://haredo.io",
    "keywords": [
        "rabbit",
        "rabbitmq",
        "amqp",
        "client",
        "typescript",
        "haredo"
    ],
    "main": "dist/index.js",
    "exports": {
        ".": "./dist/index.js",
        "./types": "./dist/types.js",
        "./internals": "./dist/internals.js"
    },
    "typesVersions": {
        "*": {
            "types": [
                "dist/types"
            ],
            "internals": [
                "dist/internals"
            ]
        }
    },
    "scripts": {
        "build": "npm run build:ts",
        "lint": "eslint 'src/**/*.ts'",
        "build:ts": "tsc",
        "watch:ts": "tsc -w",
        "test": "mocha 'src/**/*.test.ts'",
        "test:watch": "yarn run test --watch",
        "test:mocked:watch": "mocha 'src/**/*.test.ts' --ignore src/haredo.integration.test.ts --watch",
        "coverage": "nyc npm run test",
        "rabbitmq:start": "docker-compose -f docker/docker-compose.yml up",
        "generate-docs": "typedoc --out docs src/index.ts src/types.ts src/internals.ts"
    },
    "author": "Kristjan Tammekivi",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/KristjanTammekivi/Haredo"
    },
    "dependencies": {
        "@cloudamqp/amqp-client": "^3.4.1"
    },
    "devDependencies": {
        "@faker-js/faker": "^9.9.0",
        "@types/dotenv": "^8.2.3",
        "@types/mocha": "^10.0.8",
        "@types/node": "^24.5.1",
        "@types/sinon": "^17.0.4",
        "@typescript-eslint/eslint-plugin": "^8.44.1",
        "@typescript-eslint/parser": "^8.44.1",
        "dotenv": "^17.2.2",
        "eslint": "^9.31.0",
        "eslint-config-hein": "^2.0.0",
        "hein": "^1.5.0",
        "hein-plugin-sinon": "^1.1.1",
        "mocha": "^11.7.2",
        "nyc": "^17.1.0",
        "rabbitmq-admin": "^1.3.0",
        "rimraf": "^5.0.7",
        "sinon": "^21.0.0",
        "source-map-support": "^0.5.6",
        "ts-node": "^10.9.2",
        "typedoc": "^0.28.13",
        "typescript": "^5.8.3"
    },
    "resolutions": {
        "**/prettier": "npm:@staycool/prettier@^3.0.1"
    }
}
