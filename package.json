{
  "name": "haredo",
  "version": "1.0.0",
  "description": "Wrapper around amqplib",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run -s test:unit && npm run -s test:integration",
    "test:integration": "mocha --require ts-node/register test/integration/**/*.test.ts",
    "test:integration:watch": "mocha --require ts-node/register --watch --watch-extensions ts test/integration/**/*.test.ts",
    "test:unit": "mocha --require ts-node/register test/unit/**/*.test.ts",
    "test:unit:watch": "mocha --require ts-node/register --watch --watch-extensions ts test/unit/**/*.test.ts"
  },
  "author": "Kristjan Tammekivi",
  "license": "MIT",
  "dependencies": {
    "@types/bluebird": "^3.5.23",
    "amqplib": "^0.5.2",
    "bluebird": "^3.5.1",
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.8",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/debug": "^0.0.30",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "rabbitmq-stats": "^1.2.0",
    "source-map-support": "^0.5.6",
    "ts-node": "^7.0.0",
    "typescript": "3"
  }
}